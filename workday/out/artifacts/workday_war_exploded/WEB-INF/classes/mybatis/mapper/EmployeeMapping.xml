<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.auspex.workday.dao.mapper.EmployeeMapper">

    <resultMap id="BaseResultMap" type="com.auspex.workday.dao.model.Employee">
        <result column="employeeId" jdbcType="INTEGER" property="employeeId" />
        <result column="employeeName" jdbcType="VARCHAR" property="employeeName" />
        <result column="employeeSex" jdbcType="VARCHAR" property="employeeSex" />
        <result column="employeeAge" jdbcType="VARCHAR" property="employeeAge" />
        <result column="employeeStartDate" jdbcType="DATE" property="employeeStartDate" />
        <result column="employeeEndDate" jdbcType="DATE" property="employeeEndDate" />
    </resultMap>

    <select id="Sel" resultType="com.auspex.workday.dao.model.Employee">
        select * from employee where employeeId = #{id}
    </select>
</mapper>